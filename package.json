{
  "name": "exchanger",
  "version": "0.1.0",
  "description": "Yet another currency exchange app",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ujinho/exchanger.git"
  },
  "author": "ujihno",
  "scripts": {
    "clean": "rm -rf ./dist",
    "check:ts": "tsc -p tsconfig.json --noEmit",
    "lint:ts": "tslint src/**/*.{ts,tsx} --fix --project ./tsconfig.json",
    "lint:style": "stylint",
    "lint": "npm run lint:ts && npm run lint:style",
    "prettify": "pretty-quick",
    "test": "jest --passWithNoTests",
    "build": "npm run clean && webpack --config ./webpack/webpack.config.prod.ts",
    "start": "webpack-dev-server --config ./webpack/webpack.config.dev.ts"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix --project ./tsconfig.json",
      "jest --bail --findRelatedTests"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "npm run check:ts && npm run prettify && npm run lint:ts && stylint"
    }
  },
  "devDependencies": {
    "@hot-loader/react-dom": "^16.11.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/autoprefixer": "^9.6.1",
    "@types/axios": "^0.14.0",
    "@types/dinero.js": "^1.6.1",
    "@types/enzyme": "^3.10.4",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/moxios": "^0.4.9",
    "@types/node": "^12.12.17",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-devtools-extension": "^2.13.2",
    "@types/terser-webpack-plugin": "^2.2.0",
    "@types/webpack": "^4.41.0",
    "@types/webpack-dev-server": "^3.9.0",
    "@types/webpack-env": "^1.14.1",
    "@types/webpack-merge": "^4.1.5",
    "autoprefixer": "^9.7.3",
    "babel-runtime": "^6.26.0",
    "css-loader": "^3.3.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "extract-css-chunks-webpack-plugin": "^4.7.1",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "full-icu": "^1.3.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "moxios": "^0.4.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "prettier-tslint": "^0.4.2",
    "pretty-quick": "^2.0.1",
    "react-hot-loader": "^4.12.18",
    "react-testing-library": "^8.0.1",
    "redux-devtools-extension": "^2.13.8",
    "style-loader": "^0.23.1",
    "stylint": "^2.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "terser-webpack-plugin": "^2.3.0",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.1.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.6",
    "@bem-react/classname": "^1.5.6",
    "@types/jest": "^24.0.23",
    "axios": "^0.19.0",
    "dinero.js": "^1.6.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "storeon": "^0.9.7",
    "typescript": "^3.7.3"
  }
}
